{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "customTemplates": {},
    "settings": {
        "appearanceTools": false,
        "background":  {
            "backgroundImage": true
        },
        "blocks": {
            "core/image": {
                "lightbox": {
                    "allowEditing": true,
                    "enabled": true
                }
            }
        },
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color": {
            "background": true,
            "custom": true,
            "customDuotone": true,
            "customGradient": true,
            "defaultDuotone": true,
            "defaultGradients": true,
            "defaultPalette": true,
            "duotone": [],
            "gradients": [],
            "link": true,
            "palette":  [
                {
                    "color": "#fcfcfc",
                    "name":"Base lighter",
                    "slug": "base-lighter"
                },
                {
                    "color": "#757575",
                    "name":"Base",
                    "slug": "base"
                },
                {
                    "color": "#1b1b1b",
                    "name":"Base darker",
                    "slug": "base-darker"
                },
                {
                    "color": "#eff6fb",
                    "name":"Primary lighter",
                    "slug": "primary-lighter"
                },
                {
                    "color": "#2378c3",
                    "name":"Primary",
                    "slug": "primary"
                },
                {
                    "color": "#11181d",
                    "name":"Primary darker",
                    "slug": "primary-darker"
                },
                {
                    "color": "#ccecf2",
                    "name":"Secondary lighter",
                    "slug": "secondary-lighter"
                },
                {
                    "color": "#168092",
                    "name":"Secondary",
                    "slug": "secondary"
                },
                {
                    "color": "#111819",
                    "name":"Secondary darker",
                    "slug": "secondary-darker"
                },
                {
                    "color": "#f5e6af",
                    "name":"Accent lighter",
                    "slug": "accent-lighter"
                },
                {
                    "color": "#8a7237",
                    "name":"Accent",
                    "slug": "accent"
                },
                {
                    "color": "#1a1614",
                    "name":"Accent darker",
                    "slug": "accent-darker"
                }
            ],
            "text": true
        },
        "custom": {},
        "dimensions": {
            "minHeight": true
        },
        "layout": {
            "contentSize": "1200px",
            "wideSize": "1440px"
        },
        "position": {
            "sticky": true
        },
        "shadow": {},
        "spacing": {
            "blockGap": true,
            "margin": true,
            "padding": true,
            "spacingSizes": [
                {
                    "slug": "none",
                    "size": "0",
                    "name": "none"
                },
                {
                    "slug": "xxs",
                    "size": "1px",
                    "name": "xxs"
                },
                {
                    "slug": "xs",
                    "size": "2px",
                    "name": "xs"
                },
                {
                    "slug": "s",
                    "size": "4px",
                    "name": "s"
                },
                {
                    "slug": "m",
                    "size": "8px",
                    "name": "m"
                },
                {
                    "slug": "l",
                    "size": "16px",
                    "name": "l"
                },
                {
                    "slug": "xl",
                    "size": "32px",
                    "name": "xl"
                },
                {
                    "slug": "xxl",
                    "size": "64px",
                    "name": "xxl"
                },
                {
                    "slug": "xxxl",
                    "size": "128px",
                    "name": "xxxl"
                }
            ],
            "units": [ "%", "px", "em", "rem", "vh", "vw" ]
        },
        "typography": {
            "customFontSize": true,
            "dropCap": true,
            "fluid": false,
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "name": "Monospace",
                    "slug": "monospace"
                },
                {
                    "fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
                    "name": "Primary",
                    "slug": "primary"
                },
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif",
                    "name": "Secondary",
                    "slug": "secondary"
                }
            ],
            "fontSizes": [
                {
                    "slug": "tiny",
                    "size": "12px",
                    "name": "tiny"
                },
                {
                    "slug": "small",
                    "size": "16px",
                    "name": "small"
                },
                {
                    "slug": "regular",
                    "size": "20px",
                    "name": "regular"
                },
                {
                    "slug": "medium",
                    "size": "24px",
                    "name": "medium"
                },
                {
                    "slug": "large",
                    "size": "28px",
                    "name": "large"
                },
                {
                    "slug": "huge",
                    "size": "32px",
                    "name": "huge"
                }
            ],
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textColumns": true,
            "textDecoration": true,
            "textTransform": true,
            "writingMode": true
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "blocks": {},
        "color": {
            "background": "var:preset|color|base-lighter",
            "text": "var:preset|color|base-darker"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "var:preset|font-size|huge"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var:preset|font-size|large"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var:preset|font-size|medium"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var:preset|font-size|regular"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var:preset|font-size|small"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var:preset|font-size|tiny"
                }
            },
            "link": {
                ":hover": {
                    "typography": {
                        "textDecoration": "none"
                    }
                },
                "color": {
                    "text": "var:preset|color|accent"
                }
            }
        },
        "spacing": {
            "blockGap": "var:preset|spacing|none",
            "padding": {
                "top": "0",
                "bottom": "0",
                "left": "0",
                "right": "0"
            }
        },
        "typography": {
            "lineHeight": 1.5,
            "fontFamily": "var:preset|font-family|secondary",
            "fontSize": "var:preset|font-size|small"
        }
    },
    "patterns": [],
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "header"
        },
        {
            "area": "footer",
            "name": "footer",
            "title": "footer"
        }
    ],
    "version": 2
}
